{
  "name": "@form/react",
  "version": "1.0.0",
  "description": "React bindings for the Rezend form store.",
  "type": "module",
  "sideEffects": false,
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "lint": "tsc --noEmit",
    "test": "echo 'Run pnpm test at repository root'"
  },
  "peerDependencies": {
    "react": "^18.0.0"
  },
  "dependencies": {
    "@form/core": "workspace:^1.0.0"
  },
  "devDependencies": {
    "tsup": "^8.0.1"
  }
}
